function create_cookie(x,y,hp)
	assert(hp <= 2)
	local cookie = {
		etype="cookie",
		x=x,
		y=y,
		w=12,
		h=4,
		sprt=16,
		hp=3,
		current_hp=hp,
		snd=001,
		draw=function(self)
			local sprite=self.sprt + (self.current_hp * 2)
			spr(sprite, self.x, self.y, 2, 1)
			local clr = rnd(15)
		end,
		hitboxes={},
		hit=function(self, ball)
			local tst = collided(self, ball)
			if tst then
				local x0 = self.x
				local x1 = self.x+self.w
				local y0 = self.y
				local y1 = self.y+self.h

				local bx0 = ball.x
				local bx1 = ball.x+ball.w
				local by0 = ball.y
				local by1 = ball.y+ball.h

				local rtst = bx0 <= x1 and bx0 >= flr(x1-(self.w/10))
				local btst = by0 <= y1 and by0 >= flr(y1-(self.h/10))
				local ltst = bx1 >= x0 and bx1 <= ceil(x0+(self.w/10))
				local ttst = by1 >= y0 and by1 <= ceil(y0+(self.h/10))

				if rtst then
					local lilpush=.25
					local ckiemax = self.x+self.w

					local depth=ckiemax-ball.x

					-- this covers hitting
					-- the corner when going
					-- up right.
					-- we don't need to change
					-- the x speed because
					-- the ball is already
					-- going right
					if ball.xspeed < 0 then
						ball.xspeed*=-1
					end

					ball.x+=depth+lilpush
				end

				if btst then 
					local ckiemax= self.y+self.h
					local depth = ckiemax-ball.y
					ball.y+=depth
					ball.yspeed*=-1
				end

				if ltst then
					local depth = self.x-ball.x
					ball.x-=depth

					-- this covers hitting
					-- the corner when going
					-- up left.
					-- we don't need to change
					-- the x speed because
					-- the ball is already
					-- going left
					if ball.xspeed > 0 then
						ball.xspeed*=-1
					end
				end
				if ttst then
					local depth = self.y-ball.y
					ball.y-=depth
					ball.yspeed*=-1
				end

				sfx(self.snd)
				self.current_hp += 1
				scoreboard.cookie_hits+=1

			end


		end,
		update=function(self)
			if self.current_hp >= self.hp then
				local mx=self.x + self.w/2
				local my=self.y + self.h/2
				create_particles(50, mx, my, 1, 10, {09, 04})
				del(entities, self)
				crumbled_count+=1
			end
		end
	}

	add(entities, cookie)
end

function create_boom(x,y)
	sfx(12)
	local boom = {
		etype="boom",
		x=x,
		y=y,
		r=1,
		maxr=140,
		scaler=1,
		clr=08,
		hitlist={},
		update=function(self)
			self.r += self.scaler

			for e in all(entities) do
				if e.etype == "cookie" then
					local tst = collided_cvr(self, e)
					if tst then
						local found = false

						for h in all(self.hitlist) do
							if e == h then
								found=true
							end
						end

						if found ~= true then
							sfx(e.snd)
							e.current_hp+=1
							add(self.hitlist, e)
						end

					end
				end
			end

			if self.r > self.maxr then
				del(entities, self)
			end
		end,
		draw=function(self)
				circ(self.x, self.y, self.r, self.clr)
		end
	}

	add(entities, boom)
end

function load_lvl(lvl)
	scoreboard:clear()
	particles={}
	crumbled_count=0
	for e in all(entities) do
		if e.etype == "ball" then
			del(entities, e)
		end
	end
	ballinplay=false


	for e in all(entities) do
		if e.etype=="cookie" 
			or e.etype =="boom" then
			del(entities, e)
		elseif e.etype=="paddle" then
			e.x=e.ix
			e.y=e.iy
		end

	end
	local cookies=0
	local chunk=16 -- based on pico8 map
	local block=8 -- everythings 8x8 
	local start=lvl*chunk*block
	local final=start+chunk*block

	assert(start <=  (96 * block))
	assert(final <= (128 * block))

	for y=0,128,block do

		for x=start,final,block do

			local chunkx=x/block
			local chunky=y/block

			x1 = x - (128 * lvl)

			sprite=mget(chunkx, chunky)

			if sprite==016 then
				create_cookie(x1,y,0)
				cookies+=1
			elseif sprite==018 then
				create_cookie(x1,y,1)
				cookies+=1
			elseif sprite==020 then
				create_cookie(x1,y,2)
				cookies+=1
			end

		end

	end

	return cookies

end

function create_lifebar(jugs)
	local lifebar={
		x=0,
		y=118,
		etype="lifebar",
		sprt=032,
		jugsinit=jugs,
		jugs=jugs,
		boomin=false,
		draw=function(self) 
			local jug = 0 
			while jug < self.jugs do
				local realx=self.x+jug*8
				spr(self.sprt, realx, self.y)
				jug+=1
			end
		end,
		boom=function(self)
			self.boomin=true
			self:unlife()
		end,
		reset=function(self)
			self.jugs=self.jugsinit
		end,
		update=function(self)
			if btnp(5) and ballinplay and self.jugs > 1 then self:boom() end
			if self.jugs == 0 then
				gamestate="lose"
				self.jugs=3
				sfx(-1)
				music(-1)
				sfx(lose_snd)
				self:reset()
			end
		end,
		unlife=function(self)
			self.jugs -= 1

			local x = self.x + 8 * self.jugs 
			local y = self.y + 4

			if self.boomin and self.jugs > 0 then
				self.boomin = false
				create_particles(50,x,y,4,24,{6,7,7,7})
				create_boom(x,y)
			else 
				create_particles(50,x,y,4,24,{6,6,7,7,7,8})
			end

		end
	}

	add(entities, lifebar)
end

function create_particles(howmany, x, y, size, ttl, colors)
	assert(type(colors) == "table")
	s=rnd(size)
	for i=1,howmany do
		local p = {
			x=x, 
			y=y, 
			w=s,
			h=s,
			c=rnd(colors),
			speedx=(rnd(2))-1, 
			speedy=(rnd(2))-1,
			ttl=rnd((ttl/2))+(ttl/2),
			timealive=0,
			update=function(self)
				self.x+=self.speedx
				self.y+=self.speedy
				self.timealive += 1
				if self.timealive > self.ttl then
					del(particles, self)
				end
			end,
			draw=function(self)
				rectfill(self.x, self.y, self.x+self.w, self.y+self.h, self.c)
			end
		}

		add(particles, p)
	end
end

function create_scoreboard()
	local scoreboard = {
		paddle_hits = 0,
		cookie_hits = 0,
		deaths = 0,
		score=0,
		snd=11,
		m=0,
		total_score=0,
		clear=function(self)
			self.paddle_hits=0
			self.cookie_hits=0
			self.deaths=0
			self.total_score+=self.score
			if self.total_score >= 32766 then self.total_score = 32766 end
			self.score=0
		end,
		reset=function(self)
			self:clear()
			self.total=0
		end,
		calc= function(self)
			self.m=100-self.paddle_hits
			local d=self.deaths+1
			self.score=(self.cookie_hits * self.m)/d
			if self.score >= 32766 then self.score = 32766 end
		end,
		draw = function(self) 
			print("cookie hits : " .. self.cookie_hits,2,8,09)
			print("paddle hits : " .. self.paddle_hits,2,16,09)
			print("deaths      : " .. self.deaths, 2,24,09)
			print("--------------"    , 2,32,09)
			print("score       : " .. self.score,2,40,07)
		end
	}
	return scoreboard
end

function _init()
	gamestate="title"	
	title_snd=04
	lose_snd=13
	win_snd=14
	frame=0
	current_lvl=0
	max_lvls=3
	particles={}
	entities={}
	ballinplay=false
	create_paddle()
	create_walls()
	crumbled_count=0
	scoreboard=create_scoreboard()
	cookie_count=load_lvl(current_lvl)
	create_lifebar(max_lvls+1)
	play_music=true
	animatedtext=create_animatedtext(story,0,56,06)
end

function _update60()
	frame+=1
	if gamestate=="title"	then
		if play_music then
			sfx(title_snd)
			play_music=false
		end
		if btnp(üÖæÔ∏è) then
			sfx(-1)
			play_music=true
			gamestate="crumbler"
		end
		if btnp(‚ùé) then
			sfx(-1)
			play_music=true
			gamestate="instructions"
		end
		if btnp(‚û°Ô∏è) then animatedtext:next() end
		if btnp(‚¨ÖÔ∏è) then animatedtext:prev() end
		animatedtext:update()
	elseif gamestate=="instructions" then
		if btnp(üÖæÔ∏è) or btnp(‚ùé) then
			gamestate="crumbler"
		end
	elseif gamestate=="crumbler" then
		ballinplay=false
		for e in all(entities) do
			if e.etype == "ball" then
				ballinplay=true
			end
		end

		if play_music then
			music(0)
			play_music=false
		end
		for e in all(entities) do
			if e.update ~= nil then
				e:update()
			end
		end
		for p in all(particles) do 
			p:update()
		end
		if crumbled_count>=cookie_count then
			gamestate="translvl"
			scoreboard:calc()
			music(-1)
			sfx(-1)
			sfx(scoreboard.snd)
			play_music=true
		end
		-- boom!
	elseif gamestate=="translvl" then
		if btnp(4) or btnp(5) then
			gamestate="nextlvl"
		end
	elseif gamestate=="nextlvl" then
		current_lvl+=1
		if current_lvl < max_lvls then
			cookie_count=load_lvl(current_lvl)
			gamestate="crumbler"
		else 
			gamestate="win"
			music(-1)
			sfx(-1)
			sfx(win_snd)
		end
	elseif gamestate=="win" then
		if btnp(‚ùé) or btnp(üÖæÔ∏è) then
			current_lvl=0
			cookie_count=load_lvl(current_lvl)
			scoreboard:reset()
			for e in all(entities) do 
				if e.etype=="lifebar" then 
					e:reset()
				end
			end
			play_music=true
			gamestate="crumbler"
		end
	elseif gamestate=="lose" then
		-- the lifebar resets itself
		-- on lose ._.
		-- my bad
		current_lvl=0
		cookie_count=load_lvl(current_lvl)
		scoreboard:reset()
		if btnp(‚ùé) or btnp(üÖæÔ∏è) then
			play_music=true
			gamestate="crumbler"
		end
	end
end

