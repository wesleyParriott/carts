function create_animatedtext(txt,x,y,clr)
	local animatedtext={
		x=x,
		y=y,
		txt=txt,
		i=1,
		timealive=0,
		ttn=13000,--time til next
		clr=clr,
		next=function(self)
			if self.i < #self.txt then
				self.i+=1
				self.timealive=0
			end
		end,
		prev=function(self)
			if self.i > 1 then
				self.i-=1
				self.timealive=0
			end
		end,
		update=function(self)
			if self.i < #self.txt then
				self.timealive+=1
				if self.timealive > self.ttn then
					self.i+=1
					self.timealive=0
				end
			end
		end,
		draw=function(self)
			print(self.txt[self.i],self.x,self.y,self.clr)
		end
	}
	return animatedtext
end

function create_paddle() 
	local paddle={
		ix=56,
		iy=108,
		x=56,
		y=108,
		w=16,
		h=8,
		sprt=000,
		frame=0,
		animate=false,
		maxframe=3,
		frametimer=0,
		frametimerrate=5,
		xspeed=0,
		vel=2,
		friction=3,
		etype="paddle",
		hitsound=08,
		badsound=10,
		launchsound=09,
		update=function(self) 
			if (btnp(04)) and ballinplay ~= true and self.animate ~= true then
				self.animate=true
				sfx(self.launchsound)
			end

		 if (btnp(05) and ballinplay ~= true) then
				printt("üÖæÔ∏è to launch", 40, 120,30,10)
				sfx(self.badsound)
			end

			if self.animate then

				if self.frametimer == self.frametimerrate then
					self.frame += 1

					if self.frame > self.maxframe then
						self.frame = 0
						self.animate = false 

						local bx = self.x + (self.w / 2) - 2 
						local by = self.y - 2
						local bs = mid(-1, self.xspeed, 1)
						create_ball(bx,by,bs)
					end

					self.frametimer=0
				end

				self.frametimer+=1
			end

			if self.xspeed < 0 then
				self.xspeed+=self.vel
			elseif self.xspeed > 0 then 
				self.xspeed-=self.vel
			end
			if btn(‚û°Ô∏è) then
				self.xspeed+=self.vel
				if self.x > 112 then 
					self.x = 112
				end
			end
			if btn(‚¨ÖÔ∏è) then
				self.xspeed-=self.vel
				if self.x < 0 then
					self.x=0
				end
			end
			self.x+=self.xspeed
		end,
		draw=function(self) 
			local sprite=self.sprt+(self.frame*2)
			spr(sprite, self.x, self.y, 2, 1)
		end,
		hit=function(self,ball)

			tst = collided(self, ball)

			if tst then

				scoreboard.paddle_hits+=1

			 local midself = (self.x + (self.w / 2))
				local midball = ball.x + (ball.w / 2)
				local tvalue = (midball - self.x) / self.w
				local send = (lerp(-1, 1, tvalue))

				ball.xspeed=send

				ball.yspeed=-1

				sfx(self.hitsound)
			end
		end
	}
	add(entities, paddle)
end

function create_ball(x,y,d)
	local ball={
		x=x,
		y=y,
		w=2,
		h=2,
		etype="ball",
		xspeed=d,
		yspeed=-1,
		iframe=0,
		iframemax=0,
		diesound=02,
		update=function(self)

			if self.iframe == 0 then 

				for e in all(entities) do
					if e ~= self then
						if e.hit ~= nil then
							e:hit(self)
						end
					end
				end

			end

			self.y+=self.yspeed
			self.x+=self.xspeed

			if self.y > 128 then
				del(entities, self)
				for e in all(entities) do 
					if e.etype == "lifebar" then
						e:unlife()
						sfx(self.diesound)
						scoreboard.deaths+=1
					end
				end
			end

			if self.iframe > 0 then 
				self.iframe -= 1 
			end
		end,
		draw=function(self)
			circfill(self.x, self.y, self.w, 6)			
			circ(self.x, self.y, self.w, 1)			
		end
	}
	add(entities, ball)
end

