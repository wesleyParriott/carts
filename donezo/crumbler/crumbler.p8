function printd(msg)
	printh("[" .. frame .. "] " .. msg)
end

function printt(str, x, y, ttl, clr)
	local printt = {
		etype="timedprint",
		x=x,
		y=y,
		timealive=0,
		update=function(self)
			if self.timealive > ttl then
				del(entities, self)
				return true
			end
			self.timealive += 1
		end,
		draw=function(self)
			print(str,self.x,self.y,clr)
		end
	}

	add(entities, printt)
end

function lerp(v0, v1, tm)
	local ret = (1 - tm) * v0 + tm * v1
	return ret
end

function collided(a,b)
	local x0=a.x
	local y0=a.y
	local w0=a.w
	local h0=a.h
	local x1=b.x
	local y1=b.y
	local w1=b.w
	local h1=b.h

	local x0max=x0+w0
	local y0max=y0+h0

	local x1max=x1+w1
	local y1max=y1+h1

	local test=(x0 < x1max and
	      x0max > x1 and
							y0 < y1max and
							y0max > y1)

	return test	
end

function collided_cvr(circle, rectangle)
	local tx = circle.x
	local ty = circle.y

	if circle.x < rectangle.x then
		tx = rectangle.x
	elseif circle.x > rectangle.x+rectangle.w then
		tx = rectangle.x+rectangle.w
	end
	
	if circle.y < rectangle.y then
		ty = circle.x
	elseif circle.y > rectangle.y+rectangle.h then
		ty = rectangle.y+rectangle.h
	end

	local dx = circle.x-tx
	local dy = circle.y-ty
	local d  = sqrt((dx*dx)+(dy*dy))

	if d <= circle.r then
		return true
	end

	return false
end

